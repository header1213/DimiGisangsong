{
  "name": "DimiGisangsong",
  "description": "Build an Extension!",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./images/dimi16.png",
      "32": "./images/dimi32.png",
      "48": "./images/dimi48.png",
      "128": "./images/dimi128.png"
    }
  },
  "icons": {
    "16": "./images/dimi16.png",
    "32": "./images/dimi32.png",
    "48": "./images/dimi48.png",
    "128": "./images/dimi128.png"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["*://*.dimigo.life/*"]
}
